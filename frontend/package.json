    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.7.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^7.0.1",
    "eslint-teamcity": "^2.0.0",
    "file-loader": "^0.11.1",
    "jest": "^21.2.1",
    "jest-teamcity-reporter": "^0.7.0",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^4.0.0-beta.15",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",

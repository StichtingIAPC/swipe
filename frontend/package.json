{
  "name": "SwiFE",
  "version": "0.0.1",
  "description": "Swipe Frontend Environment",
  "main": "index.js",
  "scripts": {
    "build": "yarn --pure-lockfile && webpack -p",
    "dev": "yarn --pure-lockfile && webpack-dev-server --inline --hot --history-api-fallback",

{
  "name": "SwiFE",
  "version": "0.0.1",
  "description": "Swipe Frontend Environment",
  "main": "index.js",
  "scripts": {
    "build": "yarn --pure-lockfile && webpack",
    "dev": "yarn --pure-lockfile && webpack-dev-server --inline --hot --history-api-fallback",
    "lint": "yarn --pure-lockfile && eslint src",
    "lint-fix": "eslint src --fix",
    "storybook": "NODE_PATH=src start-storybook -p 9001",
    "test": "jest unit --watch",
    "test-once": "jest unit"
  },
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter",
    "moduleNameMapper": {
      "\\.(css|less|scss|styl|sss)$": "<rootDir>/jest.js"
    }
  },
  "contributors": [
    "Rick Lubbers <rlubbers@iapc.utwente.nl>",
    "Matthias van de Meent <mmeent@iapc.utwente.nl>"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@react-bootstrap/pagination": "^1.0.0",
    "admin-lte": "^2.3.11",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "moment": "^2.20.1",
    "numeral": "^2.0.6",
    "polished": "^1.9.2",
    "prop-types": "^15.5.10",
    "re-reselect": "1.0.1",
    "react": "16.2",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-table-next": "^0.1.1",
    "react-datepicker": "^0.55.0",
    "react-dom": "~16.2",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.5",
    "react-router": "4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-select": "^1.2.1",
    "reactjs-admin-lte": "^0.6.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.3",
    "reselect": "^3.0.1",
    "styled-components": "^3.2.6",
    "styled-system": "^2.2.5",
    "sweetalert": "^2.1.0",
    "system-components": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.5",
    "babel-core": "^6.24.1",
    "babel-eslint": "8",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.0.0",
